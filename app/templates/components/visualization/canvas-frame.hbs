<Visualization::canvasToolbar @label={{this.label}}
  @property={{this.property}}
  @searchTerm={{this.searchTerm}}
  @loaded={{this.loaded}}
/>

<Visualization::graph
  @interactive=true
  @nodes={{this.items}}
  @clickedNode={{action "clickedNode"}}
  @doubleClickedNode={{action "doubleClickedNode"}}
/>
{{#if currentlySelectedNode}}
  <nodeMain @node={{this.currentlySelectedNode}} />
{{else}}
  <Visualization::toolTip />
{{/if}}

<Node::nodeHover @node={{this.nodeBeingHoveredOver}} />